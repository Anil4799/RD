<!-- Logout Script -->
<script>
	function logout() {
		document.getElementById("logoutform").action = "./LogoutServlet";
		document.getElementById("logoutform").method = "POST";
		document.getElementById("logoutform").submit();
	}
	window.history.forward();
	function noBack() {
		window.history.forward();
	}
</script>

